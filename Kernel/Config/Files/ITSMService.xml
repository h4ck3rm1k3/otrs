    <ConfigItem Name="ITSMService::Frontend::MenuModule###000-Back" Required="0" Valid="1">
        <Description Lang="en">Module to show back link in service menu.</Description>
        <Description Lang="de">Über dieses Modul wird der Zurück-Link in der Linkleiste der Service-Ansicht angezeigt.</Description>
        <Group>ITSM Core</Group>
        <SubGroup>Frontend::Agent::ITSMService::MenuModule</SubGroup>
        <Setting>
            <Hash>
                <Item Key="Module">Kernel::Output::HTML::ITSMServiceMenuGeneric</Item>
                <Item Key="Name">Back</Item>
                <Item Key="Description">Back</Item>
                <Item Key="Action"></Item>
                <Item Key="Link">$Env{"ITSMServiceLastScreenOverview"}</Item>
            </Hash>
        </Setting>
    </ConfigItem>
    <ConfigItem Name="ITSMService::Frontend::MenuModule###100-Print" Required="0" Valid="1">
        <Description Lang="en">Module to show print link in service menu.</Description>
        <Description Lang="de">Über dieses Modul wird der Drucken-Link in der Linkleiste der Service-Ansicht angezeigt.</Description>
        <Group>ITSM Core</Group>
        <SubGroup>Frontend::Agent::ITSMService::MenuModule</SubGroup>
        <Setting>
            <Hash>
                <Item Key="Module">Kernel::Output::HTML::ITSMServiceMenuGeneric</Item>
                <Item Key="Name">Print</Item>
                <Item Key="Description">Print</Item>
                <Item Key="Action">AgentITSMServicePrint</Item>
                <Item Key="Link">Action=AgentITSMServicePrint&amp;ServiceID=$QData{"ServiceID"}</Item>
                <Item Key="LinkParam">target="print"</Item>
            </Hash>
        </Setting>
    </ConfigItem>
    <ConfigItem Name="ITSMService::Frontend::MenuModule###200-Link" Required="0" Valid="1">
        <Description Lang="en">Module to show the link link in service menu.</Description>
        <Description Lang="de">Über dieses Modul wird der Verknüpfen-Link in der Linkleiste der Service-Ansicht angezeigt.</Description>
        <Group>ITSM Core</Group>
        <SubGroup>Frontend::Agent::ITSMService::MenuModule</SubGroup>
        <Setting>
            <Hash>
                <Item Key="Module">Kernel::Output::HTML::ITSMServiceMenuGeneric</Item>
                <Item Key="Name">Link</Item>
                <Item Key="Description">Link</Item>
                <Item Key="Action">AgentLinkObject</Item>
                <Item Key="Link">Action=AgentLinkObject&amp;SourceObject=Service&amp;SourceKey=$QData{"ServiceID"}</Item>
            </Hash>
        </Setting>
    </ConfigItem>
