<?xml version="1.0" encoding="iso-8859-1" ?>
<otrs_config version="1.0" init="Config">
    <CVS>$Id: ITSMLocation.xml,v 1.1 2008/06/24 14:26:45 ub Exp $</CVS>
    <ConfigItem Name="Frontend::Module###AdminITSMLocation" Required="0" Valid="1">
        <Description Lang="en">Frontend module registration for the AdminITSMLocation configuration in the admin area.</Description>
        <Description Lang="de">Frontendmodul-Registration der AdminITSMLocation Konfiguration im Admin-Bereich.</Description>
        <Group>ITSM</Group>
        <SubGroup>Frontend::Admin::ModuleRegistration</SubGroup>
        <Setting>
            <FrontendModuleReg>
                <Group>admin</Group>
                <Description>Admin</Description>
                <Title>Location</Title>
                <NavBarName>Admin</NavBarName>
                <NavBarModule>
                    <Module>Kernel::Output::HTML::NavBarModuleAdmin</Module>
                    <Name>Location</Name>
                    <Block>Block3</Block>
                    <Prio>1010</Prio>
                </NavBarModule>
            </FrontendModuleReg>
        </Setting>
    </ConfigItem>
    <ConfigItem Name="Frontend::Module###AgentITSMLocation" Required="0" Valid="1">
        <Description Lang="en">Frontend module registration for the AgentITSMLocation object in the agent interface.</Description>
        <Description Lang="de">Frontendmodul-Registration des AgentITSMLocation-Objekts im Agent-Interface.</Description>
        <Group>ITSM</Group>
        <SubGroup>Frontend::Agent::ModuleRegistration</SubGroup>
        <Setting>
            <FrontendModuleReg>
                <GroupRo>itsm-location</GroupRo>
                <Description>ITSM Location Overview</Description>
                <NavBarName>Location</NavBarName>
                <Title>Location</Title>
                <NavBar>
                    <Description>Location-Area</Description>
                    <Name>Location</Name>
                    <Type>Menu</Type>
                    <Block>ItemArea</Block>
                    <Image>itsm_location.png</Image>
                    <Link>Action=AgentITSMLocation</Link>
                    <NavBar>Location</NavBar>
                    <AccessKey></AccessKey>
                    <Prio>3300</Prio>
                </NavBar>
                <NavBar>
                    <Description>Overview</Description>
                    <Name>Overview</Name>
                    <Image>overview.png</Image>
                    <Link>Action=AgentITSMLocation</Link>
                    <Type></Type>
                    <Block></Block>
                    <NavBar>Location</NavBar>
                    <AccessKey></AccessKey>
                    <Prio>100</Prio>
                </NavBar>
            </FrontendModuleReg>
        </Setting>
    </ConfigItem>
    <ConfigItem Name="Frontend::Module###AgentITSMLocationZoom" Required="0" Valid="1">
        <Description Lang="en">Frontend module registration for the AgentITSMLocationZoom object in the agent interface.</Description>
        <Description Lang="de">Frontendmodul-Registration des AgentITSMLocationZoom-Objekts im Agent-Interface.</Description>
        <Group>ITSM</Group>
        <SubGroup>Frontend::Agent::ModuleRegistration</SubGroup>
        <Setting>
            <FrontendModuleReg>
                <GroupRo>itsm-location</GroupRo>
                <Description>Location Zoom</Description>
                <Title>Zoom</Title>
                <NavBarName>Location</NavBarName>
            </FrontendModuleReg>
        </Setting>
    </ConfigItem>
    <ConfigItem Name="Frontend::Module###AgentITSMLocationPrint" Required="0" Valid="1">
        <Description Lang="en">Frontend module registration for the AgentITSMLocationPrint object in the agent interface.</Description>
        <Description Lang="de">Frontendmodul-Registration des AgentITSMLocationPrint-Objekts im Agent-Interface.</Description>
        <Group>ITSM</Group>
        <SubGroup>Frontend::Agent::ModuleRegistration</SubGroup>
        <Setting>
            <FrontendModuleReg>
                <GroupRo>itsm-location</GroupRo>
                <Description>Location Print</Description>
                <Title>Print</Title>
                <NavBarName>Location</NavBarName>
            </FrontendModuleReg>
        </Setting>
    </ConfigItem>
    <ConfigItem Name="ITSMLocation::Frontend::MenuModule###000-Back" Required="0" Valid="1">
        <Description Lang="en">Module to show back link in location menu.</Description>
        <Description Lang="de">Über dieses Modul wird der Zurück-Link in der Linkleiste der Location-Ansicht angezeigt.</Description>
        <Group>ITSM</Group>
        <SubGroup>Frontend::Agent::ITSMLocation::MenuModule</SubGroup>
        <Setting>
            <Hash>
                <Item Key="Module">Kernel::Output::HTML::ITSMLocationMenuGeneric</Item>
                <Item Key="Name">Back</Item>
                <Item Key="Description">Back</Item>
                <Item Key="Action"></Item>
                <Item Key="Link">$Env{"ITSMLocationLastScreenOverview"}</Item>
            </Hash>
        </Setting>
    </ConfigItem>
    <ConfigItem Name="ITSMLocation::Frontend::MenuModule###100-Print" Required="0" Valid="1">
        <Description Lang="en">Module to show print link in location menu.</Description>
        <Description Lang="de">Über dieses Modul wird der Drucken-Link in der Linkleiste der Lokation-Ansicht angezeigt.</Description>
        <Group>ITSM</Group>
        <SubGroup>Frontend::Agent::ITSMLocation::MenuModule</SubGroup>
        <Setting>
            <Hash>
                <Item Key="Module">Kernel::Output::HTML::ITSMLocationMenuGeneric</Item>
                <Item Key="Name">Print</Item>
                <Item Key="Description">Print</Item>
                <Item Key="Action">AgentITSMLocationPrint</Item>
                <Item Key="Link">Action=AgentITSMLocationPrint&amp;LocationID=$QData{"LocationID"}</Item>
                <Item Key="LinkParam">target="print"</Item>
            </Hash>
        </Setting>
    </ConfigItem>
    <ConfigItem Name="ITSMLocation::Frontend::MenuModule###200-Link" Required="0" Valid="1">
        <Description Lang="en">Module to show the link link in location menu.</Description>
        <Description Lang="de">Über dieses Modul wird der Verknüpfen-Link in der Linkleiste der Location-Ansicht angezeigt.</Description>
        <Group>ITSM</Group>
        <SubGroup>Frontend::Agent::ITSMLocation::MenuModule</SubGroup>
        <Setting>
            <Hash>
                <Item Key="Module">Kernel::Output::HTML::ITSMLocationMenuGeneric</Item>
                <Item Key="Name">Link</Item>
                <Item Key="Description">Link</Item>
                <Item Key="Action">AgentLinkObject</Item>
                <Item Key="Link">Action=AgentLinkObject&amp;SourceObject=ITSMLocation&amp;SourceKey=$QData{"LocationID"}</Item>
            </Hash>
        </Setting>
    </ConfigItem>
</otrs_config>
