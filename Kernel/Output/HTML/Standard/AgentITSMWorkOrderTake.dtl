<fieldset class="TableLike W300px">
    <label>$Text{"Title"}:</label>
    <p class="Value">
        $QData{"WorkOrderTitle"}
    </p>
    <div class="Clear"></div>

    <label>$Text{"Current Agent"}:</label>
    <p class="Value">
        $QData{"WorkOrderAgent"}
    </p>
    <div class="Clear"></div>

</fieldset>
<p class="SpacingTop Center">
    $Text{"Do you really want to take this workorder?"}
</p>
