<table border="0" width="100%" cellspacing="0" cellpadding="3">
    <tr>
        <td class="mainhead">
            <div title="$Env{"Box0"}$Text{"Imperative::Take Workorder"}#: $QData{"ChangeNumber"}-$QData{"WorkOrderNumber"} $Env{"Box1"}">
                $Env{"Box0"}$Text{"Imperative::Take Workorder"}#: $QData{"ChangeNumber"}-$QData{"WorkOrderNumber"} $Env{"Box1"}
            </div>
        </td>
    </tr>
    <tr>
        <td class="menu">
            <a href="$Env{"Baselink"}$Env{"LastWorkOrderView"}" onmouseover="window.status='$JSText{"Back"}'; return true;" onmouseout="window.status='';" class="menuitem">$Text{"Back"}</a>
        </td>
    </tr>

    <tr>
        <td class="mainbody">
            <br>
            <table border="0" width="350" align="center" cellspacing="0" cellpadding="4">
                <tr>
                    <td class="contenthead">$Text{"Imperative::Take Workorder"}# $QData{"ChangeNumber"}-$QData{"WorkOrderNumber"}</td>
                </tr>
                <tr class="contentbody">
                    <td class="contentbody">
                        <table border="0" width="100%" cellspacing="0" cellpadding="3">
                            <colgroup>
                                <col width="130">
                                <col>
                            </colgroup>
                            <tr>
                                <td class="contentkey">$Text{"Title"}:</td>
                                <td class="contentvalue">$QData{"WorkOrderTitle"}</td>
                            </tr>
                            <tr>
                                <td class="contentkey">$Text{"Current Agent"}:</td>
                                <td class="contentvalue">$QData{"WorkOrderAgent"}</td>
                            </tr>
                            <tr>
                                <td colspan="2">&nbsp;</td>
                            </tr>
                            <tr>
                                <td colspan="2" class="contentkey">$Text{"Do you really want to take this workorder?"}</td>
                            </tr>
                        </table>
                    </td>
                </tr>
                <tr>
                    <td class="contentfooter">
                        <table width="100%" border="0" cellspacing="0" cellpadding="1">
                            <colgroup>
                                <col>
                                <col width="10">
                            </colgroup>
                            <tr>
                                <td class="contentfooter">
                                    <form action="$Env{"CGIHandle"}" method="post">
                                        <input type="hidden" name="Action"    value="AgentITSMWorkOrderZoom">
                                        <input type="hidden" name="WorkOrderID" value="$QData{"WorkOrderID"}"/>
                                        <input type="hidden" name="Subaction" value="">
                                        <input class="button" type="submit" value="$Text{"No"}"/>
                                    </form>
                                </td>
                                <td class="contentfooter">
                                    <form action="$Env{"CGIHandle"}" method="post">
                                        <input type="hidden" name="Action" value="$Env{"Action"}"/>
                                        <input type="hidden" name="Subaction" value="WorkOrderTake"/>
                                        <input type="hidden" name="WorkOrderID" value="$QData{"WorkOrderID"}"/>
                                        <input class="button" type="submit" value="$Text{"Yes"}"/>
                                    </form>
                                </td>
                            </tr>
                        </table>
                    </td>
                </tr>
            </table>
        </td>
    </tr>
</table>
