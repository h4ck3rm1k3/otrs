<table border="0" width="100%" cellspacing="0" cellpadding="3">
  <tr>
    <td class="mainhead">
      $Env{"Box0"}$Text{"Zoom"}: $Text{"SLA"}$Env{"Box1"}
    </td>
  </tr>
</table>
<table border="0" width="100%" cellspacing="0" cellpadding="3">
  <tr>
    <td width="75%" class="menu">
<!-- dtl:block:Menu -->
<!-- dtl:block:MenuItemSplit -->
      -
<!-- dtl:block:MenuItemSplit -->
<!-- dtl:block:MenuItem -->
      <a href="$Env{"Baselink"}$Data{"Link"}" $Data{"LinkParam"} onmouseover="window.status='$JSText{"$QData{"Name"}"}'; return true;" onmouseout="window.status='';" class="menuitem">$Text{"$QData{"Name"}"}</a>
<!-- dtl:block:MenuItem -->
<!-- dtl:block:Menu -->
    </td>
    <td align="right" width="25%" class="menu"></td>
  </tr>
</table>
<table border="0" width="100%" cellspacing="0" cellpadding="3">
  <tr>
    <td class="mainbody" align="center">
      <br>
      <table border="0" width="100%" cellspacing="0" cellpadding="1">
        <tr>
          <td width="20%" class="contentkey">$Text{"SLA"}: </td>
          <td class="contentvalue">$QData{"Name"}</td>
        </tr>
        <tr>
          <td width="20%" class="contentkey">$Text{"Type"}: </td>
          <td class="contentvalue">$Text{"$QData{"Type"}"}</td>
        </tr>
        <tr>
          <td width="20%" class="contentkey">$Text{"Calendar"}: </td>
          <td class="contentvalue">$Text{"$QData{"CalendarName"}"}</td>
        </tr>
        <tr>
          <td width="20%" class="contentkey">$Text{"First Response Time"}: </td>
          <td class="contentvalue">$QData{"FirstResponseTime"} $Text{"minutes"}</td>
        </tr>
        <tr>
          <td width="20%" class="contentkey">$Text{"Update Time"}: </td>
          <td class="contentvalue">$QData{"UpdateTime"} $Text{"minutes"}</td>
        </tr>
        <tr>
          <td width="20%" class="contentkey">$Text{"Solution Time"}: </td>
          <td class="contentvalue">$QData{"SolutionTime"} $Text{"minutes"}</td>
        </tr>
        <tr>
          <td width="20%" class="contentkey">$Text{"Minimum Time Between Incidents"}: </td>
          <td class="contentvalue">$QData{"MinTimeBetweenIncidents"} $Text{"minutes"}</td>
        </tr>
      </table>
<!-- dtl:block:Service -->
      <hr size="2">
      <br>
      <table width="100%" border="0" cellspacing="0" cellpadding="0">
        <tr>
          <td align="center">
            <table width="98%" border="0" cellspacing="0" cellpadding="3">
              <tr>
                <td colspan="4">
                  <b><i>$Text{"Associated Services"}</i></b>
                </td>
              </tr>
              <tr class="contenthead">
                <td class="contentkey" width="20">&nbsp;</td>
                <td class="contentkey">$Text{"Service"}</td>
                <td class="contentkey">$Text{"Type"}</td>
                <td class="contentkey">$Text{"Criticality"}</td>
                <td class="contentkey" width="150">$Text{"Changed"}</td>
              </tr>
<!-- dtl:block:ServiceRow -->
              <tr class="contentbody">
                <td class="$QData{"CssClass"}" align="right">
                  <img border="0" src="$Config{"Frontend::ImagePath"}$QData{"CurInciSignal"}.png" title="$Quote{"$Text{"$Data{"CurInciState"}"}"}" alt="$Quote{"$Text{"$Data{"CurInciState"}"}"}">
                </td>
                <td class="$QData{"CssClass"}">
                  <a href="$Env{"Baselink"}Action=AgentITSMServiceZoom&ServiceID=$QData{"ServiceID"}">
                  $QData{"Name"}
                  </a>
                </td>
                <td class="$QData{"CssClass"}">$Text{"$QData{"Type"}"}</td>
                <td class="$QData{"CssClass"}">$Text{"$QData{"Criticality"}"}</td>
                <td class="$QData{"CssClass"}" width="150">$TimeLong{"$QData{"ChangeTime"}"}</td>
              </tr>
<!-- dtl:block:ServiceRow -->
              <tr class="contentfooter">
                <td colspan="5">&nbsp;</td>
              </tr>
            </table>
          </td>
        </tr>
      </table>
<!-- dtl:block:Service -->
      <br>
    </td>
    <td width="25%" class="menu">
      <table border="0" cellspacing="1" cellpadding="0" width="100%">
        <tr>
          <td class="mainkey">$Text{"Created"}:</td>
          <td class="mainvalue">$TimeLong{"$Data{"CreateTime"}"}</td>
        </tr>
        <tr>
          <td class="mainkey">$Text{"Created by"}:</td>
          <td>
            <div title="$QData{"CreateUserLogin"} ($Quote{"$Data{"CreateUserFirstname"} $Data{"CreateUserLastname"}"})">
            $QData{"CreateUserLogin","15"} ($Quote{"$Data{"CreateUserFirstname"} $Data{"CreateUserLastname"}","15"})
            </div>
          </td>
        </tr>
        <tr>
          <td class="mainkey">$Text{"Last changed"}:</td>
          <td class="mainvalue">$TimeLong{"$Data{"ChangeTime"}"}</td>
        </tr>
        <tr>
          <td class="mainkey">$Text{"Last changed by"}:</td>
          <td>
            <div title="$QData{"ChangeUserLogin"} ($Quote{"$Data{"ChangeUserFirstname"} $Data{"ChangeUserLastname"}"})">
            $QData{"ChangeUserLogin","15"} ($Quote{"$Data{"ChangeUserFirstname"} $Data{"ChangeUserLastname"}","15"})
            </div>
          </td>
        </tr>
      </table>
    </td>
  </tr>
</table>
