<div class="LayoutPopup ARIARoleMain">

    <div class="Header">
        <h1>
            $Text{"History of"} $Config{"FAQ::FAQHook"}: $QData{"Number"} - $QData{"Title","60"}
        </h1>
        <p>
            <a href="#" class="CancelClosePopup">$Text{"Cancel & close window"}</a>
        </p>
    </div>

    <div class="Content">
        <div class="WidgetSimple">
            <div class="Header">
                <h2>
                    $Text{"History Content"}
                </h2>
            </div>
            <div class="Content">
                <table class="DataTable" summary="FAQ history data table">
                    <thead>
                        <tr>
                            <th>$Text{"Action"}</th>
                            <th>$Text{"User"}</th>
                            <th>$Text{"Createtime"}</th>
                        </tr>
                    </thead>
                    <tbody>

<!-- dtl:block:Row -->
                        <tr>
                            <td>
                                $QData{"Name"}
                            </td>
                            <td>
                                $QData{"CreatedBy"}
                            </td>
                            <td>
                                $TimeLong{"$Data{"Created"}"}
                            </td>
                        </tr>
<!-- dtl:block:Row -->

                    </tbody>
                </table>
            </div>
        </div>
    </div>
    <div class="Footer">
    </div>
</div>