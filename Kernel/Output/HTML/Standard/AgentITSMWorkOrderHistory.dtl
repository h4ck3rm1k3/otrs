<table border="0" width="100%" cellspacing="0" cellpadding="3">
    <tr>
        <td class="mainhead">
            <div title="$Env{"Box0"}$Text{"History of"} $Text{"Workorder#"}: $QData{"ChangeNumber"}-$QData{"WorkOrderNumber"}$Env{"Box1"} - $QData{"WorkOrderTitle"}">
                $Env{"Box0"}$Text{"History of"} $Text{"Workorder#"}: $QData{"ChangeNumber"}-$QData{"WorkOrderNumber"}$Env{"Box1"} - $QData{"WorkOrderTitle","70"}
            </div>
        </td>
    </tr>
    <tr>
        <td class="menu">
            <a href="$Env{"Baselink"}$Env{"LastScreenView"}" onmouseover="window.status='$JSText{"Back"}'; return true;" onmouseout="window.status='';" class="menuitem">$Text{"Back"}</a>
        </td>
    </tr>
    <tr>
        <td class="mainbody">
            <table border="0" cellspacing="0" cellpadding="2" width="100%">
                <colgroup>
                    <col>
                    <col>
                    <col width="60" align="center">
                    <col width="60" align="center">
                    <col>
                    <col>
                </colgroup>
                <tr class="contenthead">
                    <td>
                        $Text{"Action"}:
                    </td>
                    <td>
                        $Text{"Comment"}:
                    </td>
                    <td>
                        $Text{"Details"}:
                    </td>
                    <td>
                        $Text{"Workorder"}:
                    </td>
                    <td>
                        $Text{"User"}:
                    </td>
                    <td>
                        $Text{"Createtime"}:
                    </td>
                </tr>
<!-- dtl:block:Row -->
                <tr class="$QData{"css"}">
                    <td>
                        $Quote{"$Data{"HistoryType"}"}
                    </td>
                    <td>
                        <div title="$QData{"Content"}">$QData{"Content","100"}</div>
                    </td>
                    <td>
<!-- dtl:block:HistoryZoomDash -->
                        -
<!-- dtl:block:HistoryZoomDash -->
<!-- dtl:block:HistoryZoom -->
                        <a href="$Env{"Baselink"}Action=AgentITSMWorkOrderHistoryZoom&HistoryEntryID=$Data{"HistoryEntryID"}" onmouseover="window.status='$JSText{"More Info"}'; return true;" onmouseout="window.status='';">x</a>
<!-- dtl:block:HistoryZoom -->
                    </td>
                    <td>
<!-- dtl:block:Dash -->
                        -
<!-- dtl:block:Dash -->
<!-- dtl:block:WorkOrderZoom -->
                        <a href="$Env{"Baselink"}Action=AgentITSMWorkOrderZoom&WorkOrderID=$Data{"WorkOrderID"}" onmouseover="window.status='$JSText{"Zoom"}'; return true;" onmouseout="window.status='';">x</a>
<!-- dtl:block:WorkOrderZoom -->
                    </td>
                    <td>
                        <div title="$Quote{"$Data{"UserLogin"} ($Data{"UserFirstname"} $Data{"UserLastname"})"}">
                            $Quote{"$Data{"UserLogin"}"} ($Quote{"$Data{"UserFirstname"} $Data{"UserLastname"}","20"})
                        </div>
                    </td>
                    <td>
                        $TimeLong{"$Data{"CreateTime"}"}
                    </td>
                </tr>
<!-- dtl:block:Row -->
                <tr class="contenthead">
                    <td colspan="6">&nbsp;</td>
                </tr>
            </table>
        </td>
    </tr>
</table>
