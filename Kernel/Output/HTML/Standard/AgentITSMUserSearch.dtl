<!-- dtl:block:UserSearchAutoComplete -->
<!-- dtl:js_on_document_complete -->
<script type="text/javascript">//<![CDATA[
    Core.Config.Set('Autocomplete.MinQueryLength', "$QData{"minQueryLength"}");
    Core.Config.Set('Autocomplete.QueryDelay', "$QData{"queryDelay"}");
    Core.Config.Set('Autocomplete.MaxResultsDisplayed', "$QData{"maxResultsDisplayed"}");
    Core.Config.Set('Autocomplete.DynamicWidth', "$Config{"ITSMChange::Frontend::UserSearchAutoComplete::DynamicWidth"}");
    Core.Config.Set('Autocomplete.SearchButtonText', "$JSText{"Search Agent"}");
//]]></script>

<!-- dtl:block:UserSearchInit -->
<script type="text/javascript">//<![CDATA[
/*global ITSM: true */
    ITSM.Agent.UserSearch.Init($("#$QData{"ItemID"}"), $QData{"ActiveAutoComplete"});
//]]></script>
<!-- dtl:block:UserSearchInit -->

<!-- dtl:js_on_document_complete -->
<!-- dtl:block:UserSearchAutoComplete -->

<!-- dtl:block:UserSearchITSMSearchAutocomplete -->
<script type="text/javascript">//<![CDATA[
    Core.Config.Set('Autocomplete.Active', $QData{"active"});
    Core.Config.Set('Autocomplete.QueryDelay', "$QData{"queryDelay"}");
    Core.Config.Set('Autocomplete.MaxResultsDisplayed', "$QData{"maxResultsDisplayed"}");
    Core.Config.Set('Autocomplete.MinQueryLength', "$QData{"minQueryLength"}");
    Core.Config.Set('Autocomplete.DynamicWidth', "$Config{"ITSMChange::Frontend::UserSearchAutoComplete::DynamicWidth"}");
    Core.Config.Set('Autocomplete.SearchButtonText', "$JSText{"Search Agent"}");
//]]></script>
<!-- dtl:block:UserSearchITSMSearchAutocomplete -->
