<!-- dtl:block:Overview -->
<div class="MainBox AriaRoleMain SpacingTop">
    <div class="W75pc SpacingBottom CenterBox">
        <div class="WidgetSimple">
            <div class="Header">
                <h2>$Text{"Overview"}: $Text{"SLA"}</h2>
            </div>
            <div class="Content SpacingBottom">
                <table class="DataTable SpacingTop">
                    <thead>
                        <tr>
                            <th>$Text{"SLA"}</th>
                            <th>$Text{"Type"}</th>
                            <th>$Text{"Changed"}</th>
                        </tr>
                    </thead>
                    <tbody>
<!-- dtl:block:OverviewRow -->
                        <tr class="MasterAction">
                            <td>
                                <a class="MasterActionLink" href="$Env{"Baselink"}Action=AgentITSMSLAZoom;SLAID=$QData{"SLAID"}">
                                    $QData{"Name"}
                                </a>
                            </td>
                            <td>$Text{"$Data{"Type"}"}</td>
                            <td>$TimeLong{"$QData{"ChangeTime"}"}</td>
                        </tr>
<!-- dtl:block:OverviewRow -->
<!-- dtl:block:NoDataFoundMsg -->
                        <tr>
                            <td colspan="3">
                                $Text{"No data found."}
                            </td>
                        </tr>
<!-- dtl:block:NoDataFoundMsg -->
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>
<!-- dtl:block:Overview -->

<!-- dtl:js_on_document_complete -->
<script type="text/javascript">//<![CDATA[
    $('.MasterAction').bind('click', function (Event) {
        var $MasterActionLink = $(this).find('.MasterActionLink');
        // only act if the link was not clicked directly
        if (Event.target !== $MasterActionLink.get(0)) {
            window.location = $MasterActionLink.attr('href');
            return false;
        }
    });
//]]></script>
<!-- dtl:js_on_document_complete -->
