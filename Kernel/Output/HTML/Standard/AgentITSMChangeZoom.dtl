                                <td>$Data{"Filesize"}</td>
                            </tr>
<!-- dtl:block:AttachmentRow -->
<!-- empty row, for having a valid HTML table in the absence of attachments -->
                            <tr><td colspan="3"></td></tr>
                        </table>
                    </td>
                </tr>
                <tr><td colspan="2">&nbsp;</td></tr>
