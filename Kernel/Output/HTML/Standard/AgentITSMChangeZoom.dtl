            <table border="0" width="100%" cellspacing="0" cellpadding="0">
                <tr>
                    <td>&nbsp;</td>
                </tr>
<!-- dtl:block:WorkOrderGraph -->
                <tr>
                    <td>
                        $Data{"WorkOrderGraph"}
                    </td>
                </tr>
<!-- dtl:block:WorkOrderGraph -->
            </table>
            <hr size="2">
            <table border="0" width="100%" cellspacing="0" cellpadding="1">
                <colgroup>
                    <col width="130">
                    <col>
                </colgroup>
                <tr>
                    <td class="contentkey">
                        <div title="$Text{"Description"}">
                            $Text{"Description"}:&nbsp;&nbsp;&nbsp;&nbsp;
                        </div>
                    </td>
                    <td class="contentvalue">
                        $Data{"Description"}
                    </td>
                </tr>
                <tr><td colspan="2">&nbsp;</td></tr>
                <tr>
                    <td class="contentkey">
                        <div title="$Text{"Justification"}">
                            $Text{"Justification"}:&nbsp;&nbsp;&nbsp;&nbsp;
