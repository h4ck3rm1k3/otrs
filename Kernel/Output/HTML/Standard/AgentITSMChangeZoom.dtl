<table border="0" width="100%" cellspacing="0" cellpadding="3">
    <tr>
        <td class="mainhead">
            <div title="$Env{"Box0"} $Text{"Zoom"} $Config{"ITSMChange::Hook"}: $QData{"ChangeNumber"} $Env{"Box1"} $QData{"ChangeTitle"}">
                $Env{"Box0"} $Text{"Zoom"} $Config{"ITSMChange::Hook"}: $QData{"ChangeNumber"} $Env{"Box1"} $QData{"ChangeTitle","70"}
            </div>
        </td>
    </tr>
</table>
<table border="0" width="100%" cellspacing="0" cellpadding="3">
    <tr>
        <td width="75%" class="menu">
<!-- dtl:block:Menu -->
<!-- dtl:block:MenuItemSplit -->
            -
<!-- dtl:block:MenuItemSplit -->
<!-- dtl:block:MenuItem -->
            <a href="$Env{"Baselink"}$Data{"Link"}" $Data{"LinkParam"} onmouseover="window.status='$JSText{"$QData{"Name"}"}'; return true;" onmouseout="window.status='';" class="menuitem" title="$Text{"$QData{"Description"}"}">$Text{"$QData{"Name"}"}</a>
<!-- dtl:block:MenuItem -->
<!-- dtl:block:Menu -->
        </td>
        <td align="right" width="25%" class="menu"></td>
    </tr>
</table>
<table border="0" width="100%" cellspacing="0" cellpadding="3">
    <colgroup>
        <col width="75%">
        <col width="25%">
    </colgroup>
    <tr>
        <td valign="top" class="mainbody">
            <table border="0" width="100%" cellspacing="0" cellpadding="0">
                <tr>
                    <td>&nbsp;</td>
                </tr>
<!-- dtl:block:WorkOrderGraph -->
                <tr>
                    <td>
                        $Data{"WorkOrderGraph"}
                    </td>
                </tr>
<!-- dtl:block:WorkOrderGraph -->
            </table>
            <hr size="2">
            <table border="0" width="100%" cellspacing="0" cellpadding="1">
                <colgroup>
                    <col width="130">
                    <col>
                </colgroup>
                <tr>
                    <td class="contentkey">
                        <div title="$Text{"Description"}">
                            $Text{"Description"}:&nbsp;&nbsp;&nbsp;&nbsp;
                        </div>
                    </td>
                    <td class="contentvalue">
                        $Data{"Description"}
                    </td>
                </tr>
                <tr><td colspan="2">&nbsp;</td></tr>
                <tr>
                    <td class="contentkey">
                        <div title="$Text{"Justification"}">
                            $Text{"Justification"}:&nbsp;&nbsp;&nbsp;&nbsp;
                        </div>
                    </td>
                    <td class="contentvalue">
                        $Data{"Justification"}
                    </td>
                </tr>
                <tr><td colspan="2">&nbsp;</td></tr>
                <tr>
                    <td class="contentkey" style="vertical-align: top">
                        <div title="$Text{"Attachment"}">
                            $Text{"Attachment"}:&nbsp;&nbsp;&nbsp;&nbsp;
                        </div>
                    </td>
                    <td class="contentvalue">
                        <table>
                            <colgroup>
                                <col width="250">
                                <col width="16">
                                <col width="100">
                                <col>
                            </colgroup>
<!-- dtl:block:AttachmentRow -->
                            <tr>
                                <td>$Data{"Filename"}</td>
                                <td><a href="$Env{"Baselink"}Action=$Env{"Action"};Subaction=DownloadAttachment;FileID=$QData{"FileID"};ChangeID=$QData{"ChangeID"}"><img src="$Config{"Frontend::ImagePath"}disk-s.png" border="0" alt="Download" title="Download"/></a></td>
                                <td>$Data{"Filesize"}</td>
                            </tr>
<!-- dtl:block:AttachmentRow -->
<!-- empty row, for having a valid HTML table in the absence of attachments -->
                            <tr><td colspan="3"></td></tr>
                        </table>
                    </td>
                </tr>
                <tr><td colspan="2">&nbsp;</td></tr>
            </table>
<!-- dtl:block:LinkTableComplex -->
            <hr size="2">
            <br>
            <table width="98%" align="center" border="0" cellspacing="0" cellpadding="0">
                <tr>
                    <td>
                        $Data{"LinkTableStrg"}
                    </td>
                </tr>
            </table>
            <br>
<!-- dtl:block:LinkTableComplex -->
        </td>
        <td valign="top" class="menu">
<!-- dtl:block:Meta -->
            <table border="0" cellspacing="1" cellpadding="0" width="100%">
                <tr>
                    <td><b>$Text{"ChangeState"}: </b></td>
                    <td>
                        <table border="0" cellpadding="0" cellspacing="0">
                            <tbody>
                                <tr>
                                    <td>
                                        <div title="$Text{"$QData{"ChangeState"}"}">
                                            $Text{"$QData{"ChangeState"}"}&nbsp;&nbsp;
                                        </div>
                                    </td>
                                    <td>
                                        <img border="0" src="$Config{"Frontend::ImagePath"}$QData{"ChangeStateSignal"}.png" title="$Text{"$QData{"ChangeState"}"}" alt="$Text{"$QData{"ChangeState"}"}">
                                    </td>
                                </tr>
                            </tbody>
                        </table>
                    </td>
                </tr>
                <tr>
                    <td colspan="2">&nbsp;</td>
                </tr>
                <tr>
                    <td class="mainkey">$Text{"PlannedStartTime"}:</td>
                    <td class="mainvalue">
<!-- dtl:block:EmptyPlannedStartTime -->
                        <div title="-">-</div>
<!-- dtl:block:EmptyPlannedStartTime -->
<!-- dtl:block:PlannedStartTime -->
                        <div title="$QData{"PlannedStartTime"}">
                            $TimeLong{"$Data{"PlannedStartTime"}"}
                        </div>
<!-- dtl:block:PlannedStartTime -->
                    </td>
                </tr>
                <tr>
                    <td class="mainkey">$Text{"PlannedEndTime"}:</td>
                    <td class="mainvalue">
<!-- dtl:block:EmptyPlannedEndTime -->
                        <div title="-">-</div>
<!-- dtl:block:EmptyPlannedEndTime -->
<!-- dtl:block:PlannedEndTime -->
                        <div title="$QData{"PlannedEndTime"}">
                            $TimeLong{"$Data{"PlannedEndTime"}"}
                        </div>
<!-- dtl:block:PlannedEndTime -->
                    </td>
                </tr>
                <tr>
                    <td class="mainkey">$Text{"ActualStartTime"}:</td>
                    <td class="mainvalue">
<!-- dtl:block:EmptyActualStartTime -->
                        <div title="-">-</div>
<!-- dtl:block:EmptyActualStartTime -->
<!-- dtl:block:ActualStartTime -->
                        <div title="$QData{"ActualStartTime"}">
                            $TimeLong{"$Data{"ActualStartTime"}"}
                        </div>
<!-- dtl:block:ActualStartTime -->
                    </td>
                </tr>
                <tr>
                    <td class="mainkey">$Text{"ActualEndTime"}:</td>
                    <td class="mainvalue">
<!-- dtl:block:EmptyActualEndTime -->
                        <div title="-">-</div>
<!-- dtl:block:EmptyActualEndTime -->
<!-- dtl:block:ActualEndTime -->
                        <div title="$QData{"ActualEndTime"}">
                            $TimeLong{"$Data{"ActualEndTime"}"}
                        </div>
<!-- dtl:block:ActualEndTime -->
                    </td>
                </tr>
<!-- dtl:block:RequestedTime -->
                <tr>
                    <td colspan="2">&nbsp;</td>
                </tr>
                <tr>
                    <td class="mainkey">$Text{"Requested (by customer) Date"}:</td>
                    <td class="mainvalue">
                        <div title="$QData{"RequestedTime"}">$TimeLong{"$Data{"RequestedTime"}"}</div>
                    </td>
                </tr>
<!-- dtl:block:RequestedTime -->
<!-- dtl:block:EmptyRequestedTime -->
                <tr>
                    <td colspan="2">&nbsp;</td>
                </tr>
                <tr>
                    <td class="mainkey">$Text{"Requested (by customer) Date"}:</td>
                    <td class="mainvalue">
                        <div title="-">-</div>
                    </td>
                </tr>
<!-- dtl:block:EmptyRequestedTime -->
                <tr>
                    <td colspan="2">&nbsp;</td>
                </tr>
<!-- dtl:block:ShowPlannedEffort -->
                <tr>
                    <td><b>$Text{"PlannedEffort"}: </b></td>
                    <td>
<!-- dtl:block:EmptyPlannedEffort -->
                        <div title="-">
                            -
                        </div>
<!-- dtl:block:EmptyPlannedEffort -->
<!-- dtl:block:PlannedEffort -->
                        <div title="$QData{"PlannedEffort"}">
                            $QData{"PlannedEffort"}
                        </div>
<!-- dtl:block:PlannedEffort -->
                    </td>
                </tr>
<!-- dtl:block:ShowPlannedEffort -->
<!-- dtl:block:ShowAccountedTime -->
                <tr>
                    <td><b>$Text{"AccountedTime"}: </b></td>
                    <td>
<!-- dtl:block:EmptyAccountedTime -->
                        <div title="-">
                            -
                        </div>
<!-- dtl:block:EmptyAccountedTime -->
<!-- dtl:block:AccountedTime -->
                        <div title="$QData{"AccountedTime"}">
                            $QData{"AccountedTime"}
                        </div>
<!-- dtl:block:AccountedTime -->
                    </td>
                </tr>
<!-- dtl:block:ShowAccountedTime -->
                <tr>
                    <td colspan="2">&nbsp;</td>
                </tr>
<!-- dtl:block:Category -->
                <tr valign="top">
                    <td class="mainkey">$Text{"Category"}:</td>
                    <td class="mainvalue">
                        <div title="$Text{"$Data{"Category"}"}">$Text{"$Data{"Category"}"}</div>
                    </td>
                </tr>
<!-- dtl:block:Category -->
<!-- dtl:block:Impact -->
                <tr valign="top">
                    <td class="mainkey">$Text{"Impact"}:</td>
                    <td class="mainvalue">
                        <div title="$Text{"$Data{"Impact"}"}">$Text{"$Data{"Impact"}"}</div>
                    </td>
                </tr>
<!-- dtl:block:Impact -->
<!-- dtl:block:Priority -->
                <tr valign="top">
                    <td class="mainkey">$Text{"Priority"}:</td>
                    <td class="mainvalue">
                        <div title="$Text{"$Data{"Priority"}"}">$Text{"$Data{"Priority"}"}</div>
                    </td>
                </tr>
<!-- dtl:block:Priority -->
                <tr>
                    <td colspan="2">&nbsp;</td>
                </tr>
                <tr>
                    <td class="mainkey">$Text{"Change Initiator(s)"}:</td>
                    <td class="mainvalue">
<!-- dtl:block:EmptyChangeInitiators -->
                        <div title="-">-</div>
<!-- dtl:block:EmptyChangeInitiators -->
<!-- dtl:block:ChangeInitiatorExists -->
                        <div title="$QData{"Change Initiators"}">
<!-- dtl:block:ChangeInitiator -->
                            <div title="$QData{"UserLogin"} ($Quote{"$Data{"UserFirstname"} $Data{"UserLastname"}"})">
                                $QData{"UserLogin","15"} ($Quote{"$Data{"UserFirstname"} $Data{"UserLastname"}","15"})
                            </div>
<!-- dtl:block:ChangeInitiator -->
                        </div>
<!-- dtl:block:ChangeInitiatorExists -->
                    </td>
                </tr>
                <tr>
                    <td colspan="2">&nbsp;</td>
                </tr>
                <tr>
                    <td class="mainkey">$Text{"ChangeManager"}:</td>
                    <td class="mainvalue">
<!-- dtl:block:EmptyChangeManager -->
                        <div title="-">-</div>
<!-- dtl:block:EmptyChangeManager -->
<!-- dtl:block:ChangeManager -->
                        <div title="$QData{"ChangeManagerUserLogin"} ($Quote{"$Data{"ChangeManagerUserFirstname"} $Data{"ChangeManagerUserLastname"}"})">
                            $QData{"ChangeManagerUserLogin","15"} ($Quote{"$Data{"ChangeManagerUserFirstname"} $Data{"ChangeManagerUserLastname"}","15"})
                        </div>
<!-- dtl:block:ChangeManager -->
                    </td>
                </tr>
                <tr>
                    <td colspan="2">&nbsp;</td>
                </tr>
                <tr>
                    <td class="mainkey">$Text{"ChangeBuilder"}:</td>
                    <td class="mainvalue">
                        <div title="$QData{"ChangeBuilderUserLogin"} ($Quote{"$Data{"ChangeBuilderUserFirstname"} $Data{"ChangeBuilderUserLastname"}"})">
                            $QData{"ChangeBuilderUserLogin","15"} ($Quote{"$Data{"ChangeBuilderUserFirstname"} $Data{"ChangeBuilderUserLastname"}","15"})
                        </div>
                    </td>
                </tr>
                <tr>
                    <td colspan="2">&nbsp;</td>
                </tr>
                <tr>
                    <td class="mainkey">$Text{"CAB"}:</td>
                    <td class="mainvalue">
<!-- dtl:block:EmptyCAB -->
                        <div title="-">-</div>
<!-- dtl:block:EmptyCAB -->
<!-- dtl:block:CAB -->
                        <div title="$QData{"Change Advisory Board"}">
<!-- dtl:block:CABAgent -->
                            <div title="$QData{"CABAgentUserLogin"} ($Quote{"$Data{"CABAgentUserFirstname"} $Data{"CABAgentUserLastname"}"})">$QData{"CABAgentUserLogin","15"} ($Quote{"$Data{"CABAgentUserFirstname"} $Data{"CABAgentUserLastname"}","15"})</div>
<!-- dtl:block:CABAgent -->
<!-- dtl:block:CABCustomer -->
                            <div title="$QData{"CABCustomerUserLogin"} ($Quote{"$Data{"CABCustomerUserFirstname"} $Data{"CABCustomerUserLastname"}"})">$QData{"CABCustomerUserLogin","15"} ($Quote{"$Data{"CABCustomerUserFirstname"} $Data{"CABCustomerUserLastname"}","15"})</div>
<!-- dtl:block:CABCustomer -->
                        </div>
<!-- dtl:block:CAB -->
                    </td>
                </tr>
                <tr>
                    <td colspan="2">&nbsp;</td>
                </tr>
                <tr>
                    <td class="mainkey">$Text{"Created"}:</td>
                    <td class="mainvalue">$TimeLong{"$Data{"CreateTime"}"}</td>
                </tr>
                <tr>
                    <td class="mainkey">$Text{"Created by"}:</td>
                    <td>
                        <div title="$QData{"CreateUserLogin"} ($Quote{"$Data{"CreateUserFirstname"} $Data{"CreateUserLastname"}"})">
                            $QData{"CreateUserLogin","15"} ($Quote{"$Data{"CreateUserFirstname"} $Data{"CreateUserLastname"}","15"})
                        </div>
                    </td>
                </tr>
                <tr>
                    <td class="mainkey">$Text{"Last changed"}:</td>
                    <td class="mainvalue">$TimeLong{"$Data{"ChangeTime"}"}</td>
                </tr>
                <tr>
                    <td class="mainkey">$Text{"Last changed by"}:</td>
                    <td>
                        <div title="$QData{"ChangeUserLogin"} ($Quote{"$Data{"ChangeUserFirstname"} $Data{"ChangeUserLastname"}"})">
                            $QData{"ChangeUserLogin","15"} ($Quote{"$Data{"ChangeUserFirstname"} $Data{"ChangeUserLastname"}","15"})
                        </div>
                    </td>
                </tr>
<!-- dtl:block:LinkTableSimple -->
                $Data{"LinkTableStrg"}
<!-- dtl:block:LinkTableSimple -->
            </table>
<!-- dtl:block:Meta -->
        </td>
    </tr>
</table>
