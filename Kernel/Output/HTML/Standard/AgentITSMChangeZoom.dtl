<table border="0" width="100%" cellspacing="0" cellpadding="3">
    <tr>
        <td class="mainhead">
            <div title="$Env{"Box0"} $Text{"Zoom"} $Config{"ITSMChange::Hook"}: $QData{"ChangeNumber"} $Env{"Box1"} $QData{"ChangeTitle"}">
                $Env{"Box0"} $Text{"Zoom"} $Config{"ITSMChange::Hook"}: $QData{"ChangeNumber"} $Env{"Box1"} $QData{"ChangeTitle","70"}
            </div>
        </td>
    </tr>
</table>
<table border="0" width="100%" cellspacing="0" cellpadding="3">
    <tr>
        <div class="ControlRow">
            <h2></h2>
        </div>
        <div class="ActionRow">
            <ul class="Actions">
<!-- dtl:block:Menu -->
<!-- dtl:block:MenuItem -->
                <li>
                    <a href="$Env{"Baselink"}$Data{"Link"}" class="$QData{"MenuClass"}" title="$Text{"$Data{"Description"}"}">$Text{"$Data{"Name"}"}</a>
                </li>
<!-- dtl:block:MenuItem -->
<!-- dtl:block:Menu -->
            </ul>
        </div>
    </tr>
</table>
<table border="0" width="100%" cellspacing="0" cellpadding="3">
    <colgroup>
        <col width="75%">
        <col width="25%">
    </colgroup>
    <tr>
