<table border="0" width="100%" cellspacing="0" cellpadding="3">
    <tr>
        <td class="mainhead">
            <div title="$Env{"Box0"} $Text{"Delete"} $Text{"Template"} $QData{"Name"} $Env{"Box1"}">
                $Env{"Box0"} $Text{"Delete"} $Text{"Template"} $QData{"Name"} $Env{"Box1"}
            </div>
        </td>
    </tr>
    <tr>
        <td class="menu">
            <a href="$Env{"Baselink"}$Env{"LastScreenTemplates"}" onmouseover="window.status='$JSText{"Back"}'; return true;" onmouseout="window.status='';" class="menuitem">$Text{"Back"}</a>
        </td>
    </tr>

    <tr>
        <td class="mainbody">
            <br>
            <table border="0" width="350" align="center" cellspacing="0" cellpadding="4">
                <tr>
                    <td class="contenthead">$Text{"Delete"} $Text{"Template"} $QData{"Name"}</td>
                </tr>
                <tr class="contentbody">
                    <td class="contentbody">
                        <table border="0" width="100%" cellspacing="0" cellpadding="3">
                            <colgroup>
                                <col width="130">
                                <col>
                            </colgroup>
                            <tr>
                                <td class="contentkey">$Text{"Name"}:</td>
                                <td class="contentvalue">$QData{"Name"}</td>
                            </tr>
                            <tr>
                                <td class="contentkey">$Text{"Type"}:</td>
                                <td class="contentvalue">$Text{"$QData{"Type"}"}</td>
                            </tr>
                            <tr>
                                <td class="contentkey">$Text{"Comment"}:</td>
                                <td class="contentvalue">$QData{"Comment", "40"}</td>
                            </tr>
                            <tr>
                                <td colspan="2">&nbsp;</td>
                            </tr>
                            <tr>
                                <td colspan="2" class="contentkey">$Text{"Do you really want to delete this template?"}</td>
                            </tr>
                        </table>
                    </td>
                </tr>
                <tr>
                    <td class="contentfooter">
                        <table width="100%" border="0" cellspacing="0" cellpadding="1">
                            <colgroup>
                                <col>
                                <col width="10">
                            </colgroup>
                            <tr>
                                <td class="contentfooter">
                                    <form action="$Env{"CGIHandle"}" method="post" enctype="multipart/form-data">
                                        <input type="hidden" name="Action"    value="AgentITSMTemplateOverview">
                                        <input type="hidden" name="TemplateID" value="$QData{"TemplateID"}"/>
                                        <input type="hidden" name="Subaction" value="">
                                        <input class="button" type="submit" value="$Text{"No"}"/>
                                    </form>
                                </td>
                                <td class="contentfooter">
                                    <form action="$Env{"CGIHandle"}" method="post" enctype="multipart/form-data">
                                        <input type="hidden" name="Action" value="$Env{"Action"}"/>
                                        <input type="hidden" name="Subaction" value="TemplateDelete"/>
                                        <input type="hidden" name="TemplateID" value="$QData{"TemplateID"}"/>
                                        <input class="button" type="submit" value="$Text{"Yes"}"/>
                                    </form>
                                </td>
                            </tr>
                        </table>
                    </td>
                </tr>
            </table>
        </td>
    </tr>
</table>
