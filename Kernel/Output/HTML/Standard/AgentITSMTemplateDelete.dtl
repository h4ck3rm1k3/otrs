<fieldset class="TableLike">
    <label>$Text{"Name"}:</label>
    <p class="Value">
        $QData{"Name"}
    </p>
    <div class="Clear"></div>

    <label>$Text{"Type"}:</label>
    <p class="Value">
        $Text{"$Data{"Type"}"}
    </p>
    <div class="Clear"></div>

    <label>$Text{"Comment"}:</label>
    <p class="Value">
        $QData{"Comment", "40"}
    </p>
    <div class="Clear"></div>
</fieldset>
<p class="SpacingTop">
    $Text{"Do you really want to delete this template?"}
</p>
