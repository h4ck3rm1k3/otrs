<style type="text/css">
    .nowrap {
        white-space: nowrap;
    }

    .fieldvalues {
        background-color: #ffffff;
        border: 1px solid #000000;
        padding: 4px;
    }
</style>

<table border="0" width="100%" cellspacing="0" cellpadding="3">
    <tr>
        <td class="mainhead">
            <div title="$Env{"Box0"}$Text{"History of"} $Text{"Change#"}: $QData{"ChangeNumber"}$Env{"Box1"} - $QData{"ChangeTitle"}">
                $Env{"Box0"}$Text{"History of"} $Text{"Change#"}: $QData{"ChangeNumber"}$Env{"Box1"} - $QData{"ChangeTitle","70"}
            </div>
        </td>
    </tr>
    <tr>
        <td class="menu">
            <a href="$Env{"Baselink"}$Env{"LastScreenHistory"}" onmouseover="window.status='$JSText{"Back"}'; return true;" onmouseout="window.status='';" class="menuitem">$Text{"Back"}</a>
        </td>
    </tr>
    <tr>
        <td class="mainbody">
            <br>
            <table border="0" width="800" align="center" cellspacing="0" cellpadding="3">
                <tr>
                    <td class="contenthead">
                        $Text{"Detailed history information of"} ChangeUpdate
                    </td>
                </tr>
                <tr class="contentbody">
                    <td>
                        <table border="0" width="100%" cellspacing="0" cellpadding="3">
                            <colgroup>
                                <col width="130">
                                <col>
                            </colgroup>
                            <tr>
                                <td class="contentkey nowrap">
                                    $Text{"Modified"}:
                                </td>
                                <td class="contentvalue">
                                    $QData{"Fieldname"}
                                </td>
                            </tr>
                            <tr>
                                <td class="contentkey nowrap">
                                    $Text{"Old Value"}:
                                </td>
                                <td class="contentvalue">
                                    <div class="fieldvalues">$Data{"ContentOld"}</div>
                                </td>
                            </tr>
                            <tr>
                                <td class="contentkey nowrap">
                                    $Text{"New Value"}:
                                </td>
                                <td class="contentvalue">
                                    <div class="fieldvalues">$Data{"ContentNew"}</div>
                                </td>
                            </tr>
                            <tr>
                                <td class="contentkey nowrap">
                                    $Text{"Changed"}:
                                </td>
                                <td class="contentvalue">
                                    $TimeLong{"$QData{"ChangeTime"}"}
                                </td>
                            </tr>
                            <tr>
                                <td class="contentkey nowrap">
                                    $Text{"Changed by"}:
                                </td>
                                <td class="contentvalue">
                                    <div title="$Quote{"$Data{"UserLogin"} ($Data{"UserFirstname"} $Data{"UserLastname"})"}">
                                        $Quote{"$Data{"UserLogin"}"} ($Quote{"$Data{"UserFirstname"} $Data{"UserLastname"}","40"})
                                    </div>
                                </td>
                            </tr>
                        </table>
                    </td>
                </tr>
                <tr>
                    <td class="contentfooter">
                        &nbsp;
                    </td>
                </tr>
            </table>
            <br>
        </td>
    </tr>
</table>
