<table border="0" width="100%" cellspacing="0" cellpadding="4">
    <tr>
        <td class="mainbody">
            <table border="0" width="100%" cellspacing="0" cellpadding="3">
                <tr class="contenthead" align="left">
<!-- dtl:block:RecordTemplateIDHeader -->
                    <th>$Text{"TemplateID"}</th>
<!-- dtl:block:RecordTemplateIDHeader -->
<!-- dtl:block:RecordNameHeader -->
                    <th>$Text{"Name"}<br/>
                    <a name="OverviewControl" href="$Env{"Baselink"}Action=$Env{"Action"};$Data{"LinkSort"};SortBy=Name;OrderBy=Down" onmouseover="window.status='$JSText{"sort upward"}'; return true;" onmouseout="window.status='';"><img border="0" src="$Config{"Frontend::ImagePath"}up-small.png" alt="$Text{"up"}"></a> /
                    <a name="OverviewControl" href="$Env{"Baselink"}Action=$Env{"Action"};$Data{"LinkSort"};SortBy=Name;OrderBy=Up" onmouseover="window.status='$JSText{"sort downward"}'; return true;" onmouseout="window.status='';"><img border="0" src="$Config{"Frontend::ImagePath"}down-small.png" alt="$Text{"down"}"></a>
                    </th>
<!-- dtl:block:RecordNameHeader -->
<!-- dtl:block:RecordTypeHeader -->
                    <th>$Text{"Type"}<br/>
                    <a name="OverviewControl" href="$Env{"Baselink"}Action=$Env{"Action"};$Data{"LinkSort"};SortBy=TemplateTypeID;OrderBy=Down" onmouseover="window.status='$JSText{"sort upward"}'; return true;" onmouseout="window.status='';"><img border="0" src="$Config{"Frontend::ImagePath"}up-small.png" alt="$Text{"up"}"></a> /
                    <a name="OverviewControl" href="$Env{"Baselink"}Action=$Env{"Action"};$Data{"LinkSort"};SortBy=TemplateTypeID;OrderBy=Up" onmouseover="window.status='$JSText{"sort downward"}'; return true;" onmouseout="window.status='';"><img border="0" src="$Config{"Frontend::ImagePath"}down-small.png" alt="$Text{"down"}"></a>
                    </th>
<!-- dtl:block:RecordTypeHeader -->
<!-- dtl:block:RecordCommentHeader -->
                    <th>$Text{"Comment"}</th>
<!-- dtl:block:RecordCommentHeader -->
<!-- dtl:block:RecordValidHeader -->
                    <th>$Text{"Valid"}<br/>
                    <a name="OverviewControl" href="$Env{"Baselink"}Action=$Env{"Action"};$Data{"LinkSort"};SortBy=ValidID;OrderBy=Down" onmouseover="window.status='$JSText{"sort upward"}'; return true;" onmouseout="window.status='';"><img border="0" src="$Config{"Frontend::ImagePath"}up-small.png" alt="$Text{"up"}"></a> /
                    <a name="OverviewControl" href="$Env{"Baselink"}Action=$Env{"Action"};$Data{"LinkSort"};SortBy=ValidID;OrderBy=Up" onmouseover="window.status='$JSText{"sort downward"}'; return true;" onmouseout="window.status='';"><img border="0" src="$Config{"Frontend::ImagePath"}down-small.png" alt="$Text{"down"}"></a>
                    </th>
<!-- dtl:block:RecordValidHeader -->
<!-- dtl:block:RecordDeleteHeader -->
                    <th align="center">$Text{"Delete"}</th>
<!-- dtl:block:RecordDeleteHeader -->
<!-- dtl:block:RecordCreateByHeader -->
                    <th>$Text{"CreateBy"}</th>
<!-- dtl:block:RecordCreateByHeader -->
<!-- dtl:block:RecordCreateTimeHeader -->
                    <th>$Text{"CreateTime"}<br/>
                    <a name="OverviewControl" href="$Env{"Baselink"}Action=$Env{"Action"};$Data{"LinkSort"};SortBy=CreateTime;OrderBy=Down" onmouseover="window.status='$JSText{"sort upward"}'; return true;" onmouseout="window.status='';"><img border="0" src="$Config{"Frontend::ImagePath"}up-small.png" alt="$Text{"up"}"></a> /
                    <a name="OverviewControl" href="$Env{"Baselink"}Action=$Env{"Action"};$Data{"LinkSort"};SortBy=CreateTime;OrderBy=Up" onmouseover="window.status='$JSText{"sort downward"}'; return true;" onmouseout="window.status='';"><img border="0" src="$Config{"Frontend::ImagePath"}down-small.png" alt="$Text{"down"}"></a>
                    </th>
<!-- dtl:block:RecordCreateTimeHeader -->
<!-- dtl:block:RecordChangeByHeader -->
                    <th>$Text{"ChangeBy"}</th>
<!-- dtl:block:RecordChangeByHeader -->
<!-- dtl:block:RecordChangeTimeHeader -->
                    <th>$Text{"ChangeTime"}<br/>
                    <a name="OverviewControl" href="$Env{"Baselink"}Action=$Env{"Action"};$Data{"LinkSort"};SortBy=ChangeTime;OrderBy=Down" onmouseover="window.status='$JSText{"sort upward"}'; return true;" onmouseout="window.status='';"><img border="0" src="$Config{"Frontend::ImagePath"}up-small.png" alt="$Text{"up"}"></a> /
                    <a name="OverviewControl" href="$Env{"Baselink"}Action=$Env{"Action"};$Data{"LinkSort"};SortBy=ChangeTime;OrderBy=Up" onmouseover="window.status='$JSText{"sort downward"}'; return true;" onmouseout="window.status='';"><img border="0" src="$Config{"Frontend::ImagePath"}down-small.png" alt="$Text{"down"}"></a>
                    </th>
<!-- dtl:block:RecordChangeTimeHeader -->
                </tr>
<!-- dtl:block:Record -->
                <tr class="$QData{"CssClass"}">
<!-- dtl:block:RecordTemplateID -->
                    <td>
                        <div title="$QData{"TemplateID"}">$QData{"TemplateID","40"}</div>
                    </td>
<!-- dtl:block:RecordTemplateID -->
<!-- dtl:block:RecordName -->
                    <td>
                        <a href="$Env{"Baselink"}Action=AgentITSMTemplateEdit;TemplateID=$QData{"TemplateID"}" onmouseover="window.status='$JSText{"Edit"}'; return true;" onmouseout="window.status='';" title="$QData{"Name"} (ID=$QData{"TemplateID"})">$QData{"Name"}</a>
                    </td>
<!-- dtl:block:RecordName -->
<!-- dtl:block:RecordType -->
                    <td>
                        <div title="$Text{"$QData{"TranslatableType"}"} (ID=$QData{"TypeID"})">$Text{"$QData{"TranslatableType","40"}"}</div>
                    </td>
<!-- dtl:block:RecordType -->
<!-- dtl:block:RecordComment -->
                    <td>
                        <div title="$QData{"Comment"}">$QData{"Comment","40"}</div>
                    </td>
<!-- dtl:block:RecordComment -->
<!-- dtl:block:RecordValid -->
                    <td>
                        <div title="$Text{"$QData{"Valid"}"}">$Text{"$QData{"Valid","40"}"}</div>
                    </td>
<!-- dtl:block:RecordValid -->
<!-- dtl:block:RecordDelete -->
                    <td align="center">
                        <a href="$Env{"Baselink"}Action=AgentITSMTemplateDelete;TemplateID=$QData{"TemplateID"}" onmouseover="window.status='$JSText{"Delete"}'; return true;" onmouseout="window.status='';" title="Delete: $QData{"Name"}">x</a>
                    </td>
<!-- dtl:block:RecordDelete -->
<!-- dtl:block:RecordCreateBy -->
                    <td>
                        <div title="$QData{"CreateByUserLogin"} $QData{"CreateByLeftParenthesis"}$QData{"CreateByUserFirstname"} $QData{"CreateByUserLastname"}$QData{"CreateByRightParenthesis"}">
                            $QData{"CreateByUserLogin","15"} $QData{"CreateByLeftParenthesis"}$Quote{"$Data{"CreateByUserFirstname"}", "15"} $Quote{"$Data{"CreateByUserLastname"}","15"}$QData{"CreateByRightParenthesis"}
                        </div>
                    </td>
<!-- dtl:block:RecordCreateBy -->
<!-- dtl:block:RecordCreateTime -->
                    <td>
                        <div title="$QData{"CreateTime"}">$QData{"CreateTime","40"}</div>
                    </td>
<!-- dtl:block:RecordCreateTime -->
<!-- dtl:block:RecordChangeBy -->
                    <td>
                        <div title="$QData{"ChangeByUserLogin"} $QData{"ChangeByLeftParenthesis"}$QData{"ChangeByUserFirstname"} $QData{"ChangeByUserLastname"}$QData{"ChangeByRightParenthesis"}">
                            $QData{"ChangeByUserLogin","15"} $QData{"ChangeByLeftParenthesis"}$Quote{"$Data{"ChangeByUserFirstname"}", "15"} $Quote{"$Data{"ChangeByUserLastname"}","15"}$QData{"ChangeByRightParenthesis"}
                        </div>
                    </td>
<!-- dtl:block:RecordChangeBy -->
<!-- dtl:block:RecordChangeTime -->
                    <td>
                        <div title="$QData{"ChangeTime"}">$QData{"ChangeTime","40"}</div>
                    </td>
<!-- dtl:block:RecordChangeTime -->
                </tr>
<!-- dtl:block:Record -->
                <tr class="contentfooter">
                    <td align="left" colspan="$QData{"ColumnCount"}">
                        &nbsp;
                    </td>
                </tr>
            </table>
        </td>
    </tr>
</table>
