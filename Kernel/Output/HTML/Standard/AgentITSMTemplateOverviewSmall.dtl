<table border="0" width="100%" cellspacing="0" cellpadding="4">
    <tr>
        <td class="mainbody">
            <table border="0" width="100%" cellspacing="0" cellpadding="3">
                <tr class="contenthead" align="left">
<!-- dtl:block:RecordTemplateIDHeader -->
                    <th>$Text{"TemplateID"}</th>
<!-- dtl:block:RecordTemplateIDHeader -->
<!-- dtl:block:RecordNameHeader -->
                    <th>$Text{"Name"}</th>
<!-- dtl:block:RecordNameHeader -->
<!-- dtl:block:RecordTypeIDHeader -->
                    <th>$Text{"TypeID"}</th>
<!-- dtl:block:RecordTypeIDHeader -->
<!-- dtl:block:RecordTypeHeader -->
                    <th>$Text{"Type"}</th>
<!-- dtl:block:RecordTypeHeader -->
<!-- dtl:block:RecordTranslatableTypeHeader -->
                    <th>$Text{"Type"}</th>
<!-- dtl:block:RecordTranslatableTypeHeader -->
<!-- dtl:block:RecordCommentHeader -->
                    <th>$Text{"Comment"}</th>
<!-- dtl:block:RecordCommentHeader -->
<!-- dtl:block:RecordContentHeader -->
                    <th>$Text{"Content"}</th>
<!-- dtl:block:RecordContentHeader -->
<!-- dtl:block:RecordValidIDHeader -->
                    <th>$Text{"ValidID"}</th>
<!-- dtl:block:RecordValidIDHeader -->
<!-- dtl:block:RecordValidHeader -->
                    <th>$Text{"Valid"}</th>
<!-- dtl:block:RecordValidHeader -->
<!-- dtl:block:RecordCreateByHeader -->
                    <th>$Text{"CreateBy"}</th>
<!-- dtl:block:RecordCreateByHeader -->
<!-- dtl:block:RecordCreateTimeHeader -->
                    <th>$Text{"CreateTime"}</th>
<!-- dtl:block:RecordCreateTimeHeader -->
<!-- dtl:block:RecordChangeByHeader -->
                    <th>$Text{"ChangeBy"}</th>
<!-- dtl:block:RecordChangeByHeader -->
<!-- dtl:block:RecordChangeTimeHeader -->
                    <th>$Text{"ChangeTime"}</th>
<!-- dtl:block:RecordChangeTimeHeader -->
                    <th>$Text{"Delete"}</th>
                </tr>
<!-- dtl:block:Record -->
                <tr class="$QData{"CssClass"}">
<!-- dtl:block:RecordTemplateID -->
                    <td>
                        <div title="$QData{"TemplateID"}">$QData{"TemplateID","40"}</div>
                    </td>
<!-- dtl:block:RecordTemplateID -->
<!-- dtl:block:RecordName -->
                    <td>
                        <a href="$Env{"Baselink"}Action=AgentITSMTemplateEdit&TemplateID=$QData{"TemplateID"}" onmouseover="window.status='$JSText{"Edit"}'; return true;" onmouseout="window.status='';" title="$QData{"Name"}">$QData{"Name"}</a>
                    </td>
<!-- dtl:block:RecordName -->
<!-- dtl:block:RecordTypeID -->
                    <td>
                        <div title="$QData{"TypeID"}">$QData{"TypeID","40"}</div>
                    </td>
<!-- dtl:block:RecordTypeID -->
<!-- dtl:block:RecordType -->
                    <td>
                        <div title="$QData{"Type"}">$QData{"Type","40"}</div>
                    </td>
<!-- dtl:block:RecordType -->
<!-- dtl:block:RecordTranslatableType -->
                    <td>
                        <div title="$Text{"$QData{"TranslatableType"}"}">$Text{"$QData{"TranslatableType","40"}"}</div>
                    </td>
<!-- dtl:block:RecordTranslatableType -->
<!-- dtl:block:RecordComment -->
                    <td>
                        <div title="$QData{"Comment"}">$QData{"Comment","40"}</div>
                    </td>
<!-- dtl:block:RecordComment -->
<!-- dtl:block:RecordContent -->
                    <td>
                        <div title="$QData{"Content"}">$QData{"Content","40"}</div>
                    </td>
<!-- dtl:block:RecordContent -->
<!-- dtl:block:RecordValidID -->
                    <td>
                        <div title="$QData{"ValidID"}">$QData{"ValidID","40"}</div>
                    </td>
<!-- dtl:block:RecordValidID -->
<!-- dtl:block:RecordValid -->
                    <td>
                        <div title="$Text{"$QData{"Valid"}"}">$Text{"$QData{"Valid","40"}"}</div>
                    </td>
<!-- dtl:block:RecordValid -->
<!-- dtl:block:RecordCreateBy -->
                    <td>
                        <div title="$QData{"CreateByUserLogin"} $QData{"CreateByLeftParenthesis"}$QData{"CreateByUserFirstname"} $QData{"CreateByUserLastname"}$QData{"CreateByRightParenthesis"}">
                            $QData{"CreateByUserLogin","15"} $QData{"CreateByLeftParenthesis"}$Quote{"$Data{"CreateByUserFirstname"}", "15"} $Quote{"$Data{"CreateByUserLastname"}","15"}$QData{"CreateByRightParenthesis"}
                        </div>
                    </td>
<!-- dtl:block:RecordCreateBy -->
<!-- dtl:block:RecordCreateTime -->
                    <td>
                        <div title="$QData{"CreateTime"}">$QData{"CreateTime","40"}</div>
                    </td>
<!-- dtl:block:RecordCreateTime -->
<!-- dtl:block:RecordChangeBy -->
                    <td>
                        <div title="$QData{"ChangeByUserLogin"} $QData{"ChangeByLeftParenthesis"}$QData{"ChangeByUserFirstname"} $QData{"ChangeByUserLastname"}$QData{"ChangeByRightParenthesis"}">
                            $QData{"ChangeByUserLogin","15"} $QData{"ChangeByLeftParenthesis"}$Quote{"$Data{"ChangeByUserFirstname"}", "15"} $Quote{"$Data{"ChangeByUserLastname"}","15"}$QData{"ChangeByRightParenthesis"}
                        </div>
                    </td>
<!-- dtl:block:RecordChangeBy -->
<!-- dtl:block:RecordChangeTime -->
                    <td>
                        <div title="$QData{"ChangeTime"}">$QData{"ChangeTime","40"}</div>
                    </td>
<!-- dtl:block:RecordChangeTime -->
                    <td>
                        <a href="$Env{"Baselink"}Action=AgentITSMTemplateDelete&TemplateID=$QData{"TemplateID"}" onmouseover="window.status='$JSText{"Delete"}'; return true;" onmouseout="window.status='';" title="$QData{"Name"}">x</a>
                    </td>
                </tr>
<!-- dtl:block:Record -->
                <tr class="contentfooter">
                    <td align="left" colspan="$QData{"ColumnCount"}">
                        &nbsp;
                    </td>
                </tr>
            </table>
        </td>
    </tr>
</table>
