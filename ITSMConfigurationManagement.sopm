        require Kernel::System::Group;
        $Self-&gt;{GroupObject} = Kernel::System::Group-&gt;new(%{$Self});

        # add group itsm-configitem
        $Self-&gt;{GroupObject}-&gt;GroupAdd(
            Name    =&gt; 'itsm-configitem',
            Comment =&gt; 'Group for ITSM ConfigItem mask access in the agent interface.',
            ValidID =&gt; 1,
            UserID  =&gt; 1,
        );

        # add user root to itsm-configitem admin group
        $Self-&gt;{GroupObject}-&gt;GroupMemberAdd(
            GID        =&gt; $Self-&gt;{GroupObject}-&gt;GroupLookup(Group =&gt; 'itsm-configitem'),
            UID        =&gt; 1,
            Permission =&gt; {
                ro        =&gt; 1,
                move_into =&gt; 1,
                create    =&gt; 1,
                owner     =&gt; 1,
                priority  =&gt; 1,
                rw        =&gt; 1,
            },
            UserID =&gt; 1,
        );
    </CodeInstall>
    <CodeUpgrade>
        require Kernel::System::Group;
        $Self-&gt;{GroupObject} = Kernel::System::Group-&gt;new(%{$Self});

        # add group itsm-configitem
        $Self-&gt;{GroupObject}-&gt;GroupAdd(
            Name    =&gt; 'itsm-configitem',
            Comment =&gt; 'Group for ITSM ConfigItem mask access in the agent interface.',
            ValidID =&gt; 1,
            UserID  =&gt; 1,
        );

        # add user root to itsm-configitem admin group
        $Self-&gt;{GroupObject}-&gt;GroupMemberAdd(
            GID        =&gt; $Self-&gt;{GroupObject}-&gt;GroupLookup(Group =&gt; 'itsm-configitem'),
            UID        =&gt; 1,
            Permission =&gt; {
                ro        =&gt; 1,
                move_into =&gt; 1,
                create    =&gt; 1,
                owner     =&gt; 1,
                priority  =&gt; 1,
                rw        =&gt; 1,
            },
            UserID =&gt; 1,
        );

        # create general catalog object
        use Kernel::System::GeneralCatalog;
        $Self-&gt;{GeneralCatalogObject} = Kernel::System::GeneralCatalog-&gt;new(%{$Self});

        # rename general catalog class from state to deployment state
        $Self-&gt;{GeneralCatalogObject}-&gt;ClassRename(
            ClassOld =&gt; 'ITSM::ConfigItem::State',
            ClassNew =&gt; 'ITSM::ConfigItem::DeploymentState',
        );

        # reset deployment state, if required
        # define default items
        my $NewItemData = {
            33 =&gt; {
                Name          =&gt; 'Expired',
                Functionality =&gt; 'productive',
                ValidID       =&gt; 1,
                Comment       =&gt; '',
            },
            34 =&gt; {
                Name          =&gt; 'Inactive',
                Functionality =&gt; 'postproductive',
                ValidID       =&gt; 1,
                Comment       =&gt; '',
            },
            35 =&gt; {
                Name          =&gt; 'Maintenance',
                Functionality =&gt; 'productive',
                ValidID       =&gt; 1,
                Comment       =&gt; '',
            },
            36 =&gt; {
                Name          =&gt; 'Pilot',
                Functionality =&gt; 'productive',
                ValidID       =&gt; 1,
                Comment       =&gt; '',
            },
            37 =&gt; {
                Name          =&gt; 'Planned',
                Functionality =&gt; 'preproductive',
                ValidID       =&gt; 1,
                Comment       =&gt; '',
            },
            38 =&gt; {
                Name          =&gt; 'Production',
                Functionality =&gt; 'productive',
                ValidID       =&gt; 1,
                Comment       =&gt; '',
            },
            39 =&gt; {
                Name          =&gt; 'Repair',
                Functionality =&gt; 'productive',
                ValidID       =&gt; 1,
                Comment       =&gt; '',
            },
            40 =&gt; {
                Name          =&gt; 'Retired',
                Functionality =&gt; 'postproductive',
                ValidID       =&gt; 1,
                Comment       =&gt; '',
            },
            41 =&gt; {
                Name          =&gt; 'Review',
                Functionality =&gt; 'productive',
                ValidID       =&gt; 1,
                Comment       =&gt; '',
            },
            42 =&gt; {
                Name          =&gt; 'Test/QA',
                Functionality =&gt; 'preproductive',
                ValidID       =&gt; 1,
                Comment       =&gt; '',
            },
        };

        # get functionality list
        my $FunctionalityList = $Self-&gt;{GeneralCatalogObject}-&gt;FunctionalityList(
            Class =&gt; 'ITSM::ConfigItem::DeploymentState',
        );

        my $ExistsPreproductive = 0;
        my $ExistsProductive = 0;
        my $ExistsPostproductive = 0;

        # check functionality list
        for my $Functionality (@{$FunctionalityList}) {
            if ($Functionality eq 'preproductive') {
                $ExistsPreproductive = 1;
            }
            elsif ($Functionality eq 'productive') {
                $ExistsProductive = 1;
            }
            elsif ($Functionality eq 'postproductive') {
                $ExistsPostproductive = 1;
            }
        }

        # start reset, if required
        if (!$ExistsPreproductive || !$ExistsProductive || !$ExistsPostproductive) {

            # get item list
            my $ItemList = $Self-&gt;{GeneralCatalogObject}-&gt;ItemList(
                Class   =&gt; 'ITSM::ConfigItem::DeploymentState',
                Valid   =&gt; 0,
            );

            # reset config item state
            for my $ItemID (keys %{$ItemList}) {
                if ($NewItemData-&gt;{$ItemID}) {

                    # update item
                    $Self-&gt;{GeneralCatalogObject}-&gt;ItemUpdate(
                        ItemID        =&gt; $ItemID,
                        Name          =&gt; $NewItemData-&gt;{$ItemID}-&gt;{Name},
                        Functionality =&gt; $NewItemData-&gt;{$ItemID}-&gt;{Functionality},
                        ValidID       =&gt; $NewItemData-&gt;{$ItemID}-&gt;{ValidID},
                        Comment       =&gt; $NewItemData-&gt;{$ItemID}-&gt;{Comment},
                        UserID        =&gt; 1,
                    );
                }
                else {

                    # get item data
                    my $CurrentItem = $Self-&gt;{GeneralCatalogObject}-&gt;ItemGet(
                        ItemID  =&gt; $ItemID,
                    );

                    # update item
                    $Self-&gt;{GeneralCatalogObject}-&gt;ItemUpdate(
                        ItemID        =&gt; $ItemID,
                        Name          =&gt; $CurrentItem-&gt;{Name},
                        Functionality =&gt; 'productive',
                        ValidID       =&gt; $CurrentItem-&gt;{ValidID} || 1,
                        Comment       =&gt; $CurrentItem-&gt;{Comment},
                        UserID        =&gt; 1,
                    );
                }
            }
        }
    </CodeUpgrade>
